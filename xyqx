		<div id="ssms">
			<pre>
				use master
				go
				if exists(select * from sysdatabases where name='XinDB')
					drop Database XinDB
				go
				Create Database XinDB 
				(
					name='XinDB_data',
					filename='XinDB.mdf',
					size=10,
					filegrowth=15%
				)
				log on
				(
					name='XinDB_ldata',
					filename='XinDB_log.ldf',
					size=5,
					filegrowth=1,
					maxsize=10
				)
				go
				if exists(select * from sysobjects where name='T1')
					drop Table T1
				go
				Create Table T1
				(
					no int identity(1000,1) Primary Key,
					name varchar(10) unique not null,
					pwd varchar(10) check(len(pwd)==6) not null,
					age int default(18) not null,
					sex char(2) check(sex in('男','女')) not null,
					school varchar(20) not null
				)
				go
			</pre>
		</div>
